{
  "name": "cosmicjs",
  "version": "4.1.2",
  "description": "The official client module for Cosmic. This module helps you easily add dynamic content to your website or application using the Cosmic headless CMS.",
  "keywords": [
    "headlesscms",
    "cms",
    "node",
    "content",
    "api"
  ],
  "homepage": "https://docs.cosmicjs.com",
  "bugs": {
    "url": "https://github.com/cosmicjs/cosmicjs-node/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmicjs/cosmicjs-node.git"
  },
  "license": "MIT",
  "author": "Tony Spiro <tspiro@tonyspiro.com> (http://tonyspiro.com/)",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean; webpack -p; babel src --out-dir dist --source-maps inline --copy-files",
    "clean": "rm -f ./cosmicjs.browser.min.js; rm -rf dist/*",
    "dev": "nodemon test.js && eslint test.js",
    "prepublish": "npm run build",
    "lint": "eslint src",
    "test": "mocha -u tdd test/**/*.js --reporter spec"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "archiver": "^4.0.1",
    "babel-loader": "^8.1.0",
    "blanket": "^1.2.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "express": "^4.16.3",
    "mocha": "^8.0.1",
    "multer": "^1.3.0",
    "nock": "^13.0.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">=6.0"
  }
}
