{
	"name": "cosmicjs",
	"version": "5.0.1",
	"description": "The official client module for Cosmic. This module helps you easily add dynamic content to your website or application using the Cosmic headless CMS.",
	"keywords": [
		"headlesscms",
		"cms",
		"node",
		"content",
		"api",
		"react",
		"reactjs",
		"vue",
		"vuejs",
		"nextjs"
	],
	"homepage": "https://www.cosmicjs.com",
	"bugs": {
		"url": "https://github.com/cosmicjs/cosmicjs-node/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cosmicjs/cosmicjs-node.git"
	},
	"license": "MIT",
	"author": "Tony Spiro <tony@cosmicjs.com>",
	"main": "dist/index.js",
	"types": "lib/cosmic.d.ts",
	"scripts": {
		"build": "npm run clean; babel src --out-dir dist --source-maps inline --copy-files; webpack",
		"clean": "rm -f ./cosmicjs.browser.min.js; rm -rf dist/*",
		"dev": "nodemon test.js && eslint test.js",
		"prepublish": "npm run build",
		"lint": "eslint --fix 'index.js' 'index-browser.js' 'src/**/*.js'",
		"test": "mocha -u tdd test/**/*.js --reporter spec --timeout 5000",
		"prepare": "husky install"
	},
	"dependencies": {
		"axios": "^1.2.3",
		"form-data": "^4.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.20.7",
		"@babel/core": "^7.20.12",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-env": "^7.20.2",
		"babel-loader": "^9.1.2",
		"dotenv": "^16.0.3",
		"eslint": "^8.32.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-node": "^4.1.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-webpack-plugin": "^3.2.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"engines": {
		"node": ">=12"
	},
	"lint-staged": {
		"**/*.js": [
			"npx prettier --write",
			"npx eslint --fix"
		]
	}
}
